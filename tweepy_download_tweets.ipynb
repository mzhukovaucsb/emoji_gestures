   "metadata": {},
   "outputs": [],
   "source": [
    "auth = tw.AppAuthHandler(consumer_key, consumer_secret)\n",
    "api= tw.API(auth, wait_on_rate_limit=True, wait_on_rate_limit_notify=True)"
   ]
  },
